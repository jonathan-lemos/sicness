{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
	"rules": {
		"indent": [
			true,
			"tabs",
			4
		],
		"no-bitwise": false,
		"one-line": false,
		"max-classes-per-file": false,
		"no-conditional-assignment": false,
		"arrow-parens": [true, "ban-single-arg-parens"],
		"no-string-literal": false
	},
	"rulesDirectory": []
}
